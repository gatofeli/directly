    "@types/chrome": "0.0.307",
    "@types/jsdom": "21.1.7",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@vitejs/plugin-react": "4.3.4",
    "@vitest/ui": "3.0.7",
    "globals": "15.15.0",
    "jsdom": "26.0.0",
    "typescript": "~5.7.2",
    "vite": "6.4.0",
    "vitest": "3.0.7"
  }
