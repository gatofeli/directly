{
  "name": "directly",
  "author": "Alejandro Aranda Muñoz",
  "keywords": [
    "typescript",
    "javascript",
    "vite",
    "vitest",
    "react",
    "chromium",
    "browser-extension",
    "manifest-v3",
    "search",
    "productivity"
  ],
  "private": true,
